  {
    "name": "HoopsHub",
    "version": "2.0",
    "description": "For all your basketball needs!",
    "icons":
    {
      "16": "./images/hoopshub16.png",
      "48": "./images/hoopshub48.png",
      "128": "./images/hoopshub128.png"
    },
    "action":
    {
      "default_popup": "./index.html",
      "default_title": "HoopsHub"
      
    },
    "manifest_version": 3,
    "permissions":
    [
      "alarms", 
      "notifications",
      "background"
    ],
    "background": 
    {
      "service_worker": "background.js"
    }
  }